{
  "name": "envalid",
  "version": "6.0.2",
  "description": "Validation for your environment variables",
  "main": "dist/index.js",
  "module": "dist/envalid.esm.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=8.12"
  },
  "engineStrict": true,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdx build",
    "coverage": "nyc npm test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/af/envalid"
  },
  "keywords": [
    ".env",
    "dotenv",
    "env",
    "env var",
    "environment",
    "environment variable",
    "validation"
  ],
  "devDependencies": {
    "husky": "4.3.0",
    "nyc": "^15.1.0",
    "painless": "^0.9.5",
    "tsdx": "^0.14.0",
    "typescript": "^4.0.3"
  },
  "author": "Aaron Franks",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "meant": "^1.0.2",
    "validator": "^13.1.17"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix",
      "pre-push": "npm test"
    }
  }
}
